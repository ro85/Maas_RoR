<tr class="liquidaciones">                       
  <td><%= @week_days.last %></td>           
  <td><%= @contract.sunday_start_hour.strftime('%R Hs') %></td>    
  <td><%= (@contract.sunday_end_hour.strftime('%R').last(2) == "59" ? @contract.sunday_end_hour + 60 : @contract.sunday_end_hour).strftime('%R Hs') %></td>
  <td class="text-center">
    <%= @contract.total_hour_sunday %>           
  </td>            
</tr>          