<tr class="liquidaciones">                       
  <td><%= @week_days.fifth %></td>           
  <td><%= @contract.friday_start_hour.strftime('%R Hs') %></td>    
  <td><%= (@contract.friday_end_hour.strftime('%R').last(2) == "59" ? @contract.friday_end_hour + 60 : @contract.friday_end_hour).strftime('%R Hs') %></td>
  <td class="text-center">
    <%= @contract.total_hour_friday %>           
  </td>            
</tr>          