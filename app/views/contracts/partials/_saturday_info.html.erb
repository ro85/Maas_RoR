<tr class="liquidaciones">                       
  <td><%= @week_days.first(6).last %></td>           
  <td><%= @contract.saturday_start_hour.strftime('%R Hs') %></td>    
  <td><%= (@contract.saturday_end_hour.strftime('%R').last(2) == "59" ? @contract.saturday_end_hour + 60 : @contract.saturday_end_hour).strftime('%R Hs') %></td>
  <td class="text-center">
    <%= @contract.total_hour_saturday %>           
  </td>            
</tr>          