<tr class="liquidaciones">                       
  <td><%= @week_days.third %></td>          
  <td><%= @contract.wednesday_start_hour.strftime('%R Hs') %></td>    
  <td><%= (@contract.wednesday_end_hour.strftime('%R').last(2) == "59" ? @contract.wednesday_end_hour + 60 : @contract.wednesday_end_hour).strftime('%R Hs') %></td>
  <td class="text-center">
    <%= @contract.total_hour_wednesday %>           
  </td>        
  <td>
    <!-- Button trigger modal -->   
    <% if current_user.admin? %>
      <i class="far fa-edit" data-bs-toggle="modal" data-bs-target="#editModal-<%= @contract.id %>"></i>
    <% end %>
    <%= render 'contracts/partials/modal_edit_contract' %>                                               
  </td>           
</tr>          